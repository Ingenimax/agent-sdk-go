{
  "mcpServers": {
    "awslabs.aws-api-mcp-server": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "--interactive",
        "--env",
        "AWS_REGION=us-west-2",
        "--env",
        "AWS_PROFILE=default",
        "--volume",
        "/path/to/your/.aws:/app/.aws",
        "public.ecr.aws/awslabs-mcp/awslabs/aws-api-mcp-server:latest"
      ],
      "env": {
        "AWS_PROFILE": "default",
        "AWS_REGION": "us-west-2"
      }
    },
    "kubectl-ai": {
      "command": "kubectl-ai",
      "args": [
        "--mcp-server",
        "--kubeconfig",
        "/path/to/your/.kube/config"
      ],
      "env": {}
    },
    "kubectl-ai-enhanced": {
      "command": "kubectl-ai",
      "args": [
        "--mcp-server",
        "--external-tools"
      ],
      "env": {}
    }
  }
}
